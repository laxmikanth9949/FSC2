<mvc:View displayBlock="true" controllerName="sap.support.fsc2.controller.OtherMCCReq" height="100%" xmlns:uxap="sap.uxap"
	xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.f" xmlns:semantic="sap.f.semantic"
	xmlns:layout="sap.ui.layout" xmlns:form="sap.ui.layout.form">
	<semantic:SemanticPage id="othermccreq" headerPinnable="true" headerExpanded="false" toggleHeaderOnTitleClick="true"
		preserveHeaderStateOnScroll="false" titleAreaShrinkRatio="1:1.6:1.6" showFooter="false">
		<semantic:titleHeading>
			<Title text="{i18n>appTitle}"/>
		</semantic:titleHeading>
		<semantic:titleSnappedOnMobile>
			<Title text="{i18n>appTitle}"/>
		</semantic:titleSnappedOnMobile>
		<semantic:titleCustomIconActions>
			<OverflowToolbarButton icon="sap-icon://sys-help" tooltip="Open Help" press="onOpenHelpJAM"/>
			<OverflowToolbarButton id="settingBtn" icon="sap-icon://settings" tooltip="Settings" press="onSettingPress"/>
		</semantic:titleCustomIconActions>

		<semantic:content>
			<VBox class="sapUiTinyMargin">
				<layout:HorizontalLayout allowWrapping="true">
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>createCustomerVisitRequestTitle}" subheader="" press="onCreateCCV">
						<TileContent footer="" unit="">
							<ImageContent src="sap-icon://create-form"></ImageContent>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>createCriticalPeriodCoverageRequestTitle}" subheader="" press="onCreateCPC">
						<TileContent footer="" unit="">
							<ImageContent src="sap-icon://create-form"></ImageContent>
						</TileContent>
					</GenericTile>
				</layout:HorizontalLayout>
			</VBox>
		</semantic:content>
	</semantic:SemanticPage>
</mvc:View>